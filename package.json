{
  "scripts": {
    "dev": "npm run dev:css",
    "build": "parcel build index.html",
    "dev:css": "concurrently \"tailwindcss -w -i tailwind.css -o style.css\" \"parcel index.html\""
  },
  "devDependencies": {
    "parcel": "^2.8.1",
    "process": "^0.11.10"
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4"
  }
}
